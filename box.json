{
  "alias": "kengrabber.phar",
  "chmod": "0755",
  "algorithm": "SHA512",
  "banner": "2015 by Simon Erhardt <me@rootlogin.ch>",
  "compression": "BZ2",
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "files": [
    "LICENSE"
  ],
  "stub": "stub.php",
  "directories" :[
    "app",
    "src"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "phpunit",
        "phpunit-test-case",
        "Tester",
        "Tests",
        "tests"
      ],
      "in": "vendor"
    }
  ],
  "git-version": "package_version",
  "output": "kengrabber.phar"
}